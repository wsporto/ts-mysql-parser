{
  "name": "@wsporto/ts-mysql-parser",
  "version": "0.2.0",
  "description": "A standalone, grammar-complete MySQL parser.",
  "license": "MIT",
  "repository": "wsporto/ts-mysql-parser",
  "author": "wsporto (https://github.com/wsporto)",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "typescript",
    "mysql-parser",
    "sql-parser",
    "parser",
    "mysql",
    "sql"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pub": "np",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "build-parser": "antlr4ts -Dlanguage=JavaScript -listener ./src/grammar/MySQLLexer.g4 ./src/grammar/MySQLParser.g4",
    "build": "tsc",
    "watch": "tsc -b -w",
    "clean": "rm -rf dist"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{yml,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3",
    "typescript": "^3.9.10"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.52",
    "@types/prettier": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.8",
    "jest": "^25.5.4",
    "lint-staged": "^10.5.4",
    "np": "^6.5.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2"
  }
}
