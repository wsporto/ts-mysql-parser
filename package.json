{
  "name": "@wsporto/ts-mysql-parser",
  "version": "0.4.0",
  "description": "A standalone, grammar-complete MySQL parser.",
  "license": "MIT",
  "repository": "wsporto/ts-mysql-parser",
  "author": "wsporto (https://github.com/wsporto)",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "typescript",
    "mysql-parser",
    "sql-parser",
    "parser",
    "mysql",
    "sql"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pub": "np",
    "test": "tsc -v",
    "lint": "eslint src/**/*.ts",
    "build-parser-mysql": "java -jar c:\\Javalib\\antlr-4.13.1-complete.jar -Dlanguage=TypeScript -o ./src/grammar/ -listener ./src/grammar/MySQLLexer.g4 ./src/grammar/MySQLParser.g4",
    "build-parser-sqlite": "java -jar c:\\Javalib\\antlr-4.13.1-complete.jar -Dlanguage=TypeScript -o ./src/sqlite/ -listener ./src/grammar/SQLiteLexer.g4 ./src/grammar/SQLiteParser.g4",
    "build": "tsc",
    "watch": "tsc -b -w",
    "clean": "rm -rf dist"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{yml,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "antlr4": "^4.13.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.52",
    "@types/prettier": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.8",
    "jest": "^29.7.0",
    "lint-staged": "^10.5.4",
    "np": "^10.0.5",
    "prettier": "^1.19.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2"
  }
}
